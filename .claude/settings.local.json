{
  "permissions": {
    "allow": [
      "mcp__laravel-boost__tinker",
      "mcp__laravel-boost__get-config",
      "mcp__laravel-boost__list-routes",
      "mcp__laravel-boost__read-log-entries",
      "mcp__laravel-boost__last-error",
      "Bash(php artisan route:list:*)",
      "Bash(php artisan:*)",
      "mcp__laravel-boost__database-schema",
      "mcp__laravel-boost__database-query",
      "Bash(cat:*)",
      "Bash(php:*)",
      "Bash(dir:*)",
      "Bash(findstr:*)",
      "Bash(powershell:*)",
      "mcp__laravel-boost__search-docs",
      "Bash(mkdir:*)",
      "mcp__laravel-boost__application-info",
      "Bash(mysql:*)",
      "Bash(find app/ -name \"*.php\" -type f -exec grep -l \"whatsAppSendMessage\\|Mail::to\\|sendSms\" {})",
      "Bash(vendor/bin/pint:*)",
      "Bash(find:*)",
      "Bash(del:*)",
      "Bash(if [ ! -d \"resources/views/errors\" ])",
      "Bash(then mkdir -p resources/views/errors)",
      "Bash(fi)",
      "Bash(git checkout:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(git push:*)",
      "Bash(composer:*)",
      "Bash(if [ ! -d \"database/migrations/central\" ])",
      "Bash(then mkdir -p database/migrations/central)",
      "Bash(if [ ! -d \"app/Models/Central\" ])",
      "Bash(then mkdir -p app/Models/Central)",
      "Bash(if [ ! -d \"app/Http/Controllers/Central\" ])",
      "Bash(then mkdir -p app/Http/Controllers/Central)"
    ],
    "deny": [],
    "ask": []
  },
  "enableAllProjectMcpServers": true,
  "enabledMcpjsonServers": [
    "laravel-boost"
  ]
}
